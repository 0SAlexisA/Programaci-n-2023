#include<stdio.h>

int main(){

    printf("Tabla de multiplicar \n ");

    int Numero, i;

    printf("Introduce el numero del que quieras saber la tabla de multiplicar ");
    printf("\n El numero a multiplicar es: ");
    scanf("%d", &Numero);
    printf("\n La tabla es:  ");
    for (i =1; i <= 10; i++){
    printf("\n %d * %d = %d", i ,Numero,  Numero * i);
    }
    return 0;
}
